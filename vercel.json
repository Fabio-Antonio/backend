{
    "name": "yalovibackend",
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/index.js"
      }
    ],
    "env": {
      "PORT" :"@yalovi-server-port",
      "DB_CNN":"@yalovi-server-db_cnn",
      "CORREO":"@yalovi-server-correo",
      "PASS":"@yalovi-server-pass"
    }
  }
  